 
@startuml



class Stage {
  lieu du stage
  date du stage
  theme du stage
  nom du tuteur 
  note 
}

class Entreprise {
  nom
  adresse
  siege social
}

class Etudiant {
  numINE
  nom
  prenom
  moyenne generale
}

class Enseignant{
  nom
  prenom
  specialite
}

  

Enseignant  "1" --- "0..*" Stage 
Etudiant "1" --- "1" Stage
Entreprise "1" --- "*" Stage
Etudiant "*" --- "1..*" GroupeTP
GroupeTP "*"---"1..*" GroupeTD



@enduml
  
